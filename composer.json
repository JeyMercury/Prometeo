{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "diglactic/laravel-breadcrumbs": "^8.1.0",
        "doctrine/dbal": "^3.6",
        "fruitcake/php-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.5",
        "imanghafoori/laravel-masterpass": "^2.2",
        "kylekatarnls/laravel-carbon-2": "^1.0",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "league/flysystem-aws-s3-v3": "^3.12",
        "maatwebsite/excel": "^3.1.44",
        "mpdf/mpdf": "^8.1",
        "msurguy/honeypot": "^1.2",
        "spatie/laravel-permission": "^5.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "nesbot/carbon": "^2.66",
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "beyondcode/laravel-dump-server": "^1.9",
        "filp/whoops": "^2.14",
        "laravel/pint": "^1.5",
        "laravel/sail": "^1.20",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Softeca\\Html\\": "packages/softeca/html/src",
            "Softeca\\Library\\": "packages/softeca/library",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": ["packages/softeca/html/src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
